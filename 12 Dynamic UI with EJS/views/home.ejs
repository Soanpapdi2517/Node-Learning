<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/output.css" />
    <title>Home</title>
  </head>
  <body
    class="min-h-screen bg-gradient-to-br from-blue-50 via-indigo-50 to-purple-50"
  >
    <!-- Navigation Header -->
    <nav
      class="bg-white/80 backdrop-blur-lg border-b border-white/20 shadow-lg"
    >
      <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="flex justify-between items-center h-16">
          <!-- Logo/Brand -->
          <div class="flex items-center space-x-3">
            <div
              class="w-10 h-10 bg-gradient-to-r from-rose-500 to-pink-500 rounded-full flex items-center justify-center shadow-lg"
            >
              <svg
                class="w-6 h-6 text-white"
                fill="currentColor"
                viewBox="0 0 20 20"
              >
                <path
                  d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z"
                />
              </svg>
            </div>
            <span
              class="text-xl font-bold bg-gradient-to-r from-rose-600 to-pink-600 bg-clip-text text-transparent"
              >Airbnb</span
            >
          </div>

          <!-- Navigation Links -->
          <div class="flex items-center space-x-4">
            <a
              href="/"
              class="text-gray-700 hover:text-rose-600 font-medium transition-colors duration-200 flex items-center space-x-1"
            >
              <svg class="w-5 h-5" fill="currentColor" viewBox="0 0 20 20">
                <path
                  d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z"
                />
              </svg>
              <span>Go to Home</span>
            </a>
            <a
              href="/host/add-home"
              class="bg-gradient-to-r from-rose-500 to-pink-500 hover:from-rose-600 hover:to-pink-600 text-white font-semibold py-2 px-4 rounded-lg shadow-lg hover:shadow-xl transform hover:-translate-y-1 transition-all duration-300 flex items-center space-x-1"
            >
              <svg class="w-5 h-5" fill="currentColor" viewBox="0 0 20 20">
                <path
                  fill-rule="evenodd"
                  d="M10 3a1 1 0 011 1v5h5a1 1 0 110 2h-5v5a1 1 0 11-2 0v-5H4a1 1 0 110-2h5V4a1 1 0 011-1z"
                  clip-rule="evenodd"
                />
              </svg>
              <span>Add Home</span>
            </a>
          </div>
        </div>
      </div>
    </nav>

    <!-- Main Content -->
    <div class="flex flex-col items-center justify-center p-4 pt-8">
      <!-- Main Container -->
      <div class="text-center mb-8">
        <div
          class="bg-white/80 backdrop-blur-lg rounded-3xl p-12 shadow-2xl border border-white/20 max-w-md mx-auto"
        >
          <!-- Icon -->
          <div class="mb-6">
            <div
              class="w-20 h-20 bg-gradient-to-r from-rose-500 to-pink-500 rounded-full flex items-center justify-center mx-auto shadow-lg"
            >
              <svg
                class="w-10 h-10 text-white"
                fill="currentColor"
                viewBox="0 0 20 20"
              >
                <path
                  d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z"
                />
              </svg>
            </div>
          </div>

          <!-- Heading -->
          <h1
            class="text-4xl font-bold text-gray-800 mb-4 bg-gradient-to-r from-rose-600 to-pink-600 bg-clip-text text-transparent"
          >
            Welcome To Airbnb
          </h1>

          <!-- Subtitle -->
          <p class="text-gray-600 mb-8 text-lg">
            Find your perfect stay or host your space
          </p>

          <!-- Button -->
          <a
            href="/host/add-home"
            class="inline-block bg-gradient-to-r from-rose-500 to-pink-500 hover:from-rose-600 hover:to-pink-600 text-white font-semibold py-4 px-8 rounded-full shadow-lg hover:shadow-xl transform hover:-translate-y-1 transition-all duration-300 text-lg"
          >
            Add Home
          </a>
        </div>
      </div>

      <!-- Registered Homes List -->
      <ul
        class="mt-8 max-w-md mx-auto bg-white/60 backdrop-blur-sm rounded-2xl p-6 shadow-lg border border-white/20"
      >
        <li
          class="text-lg font-bold text-gray-800 mb-4 text-center bg-gradient-to-r from-rose-600 to-pink-600 bg-clip-text text-transparent border-b border-gray-200 pb-2"
        >
          Here are your Registered Homes:
        </li>
        <%registeredHomes.forEach(home => {%>
        <li
          class="text-gray-700 font-medium py-2 px-4 bg-white/50 rounded-lg mb-2 shadow-sm border border-white/30 hover:bg-white/70 transition-all duration-200 flex items-center"
        >
          <svg
            class="w-5 h-5 text-rose-500 mr-3"
            fill="currentColor"
            viewBox="0 0 20 20"
          >
            <path
              d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z"
            />
          </svg>
          <%= home.houseName%>
        </li>
        <% });%>
      </ul>
    </div>

    <!-- Decorative elements -->
    <div
      class="absolute top-10 left-10 w-32 h-32 bg-gradient-to-r from-blue-400 to-purple-400 rounded-full opacity-20 blur-xl"
    ></div>
    <div
      class="absolute bottom-10 right-10 w-40 h-40 bg-gradient-to-r from-pink-400 to-rose-400 rounded-full opacity-20 blur-xl"
    ></div>
  </body>
</html>
